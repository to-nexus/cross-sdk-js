{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "outDir": "./dist/types",
    "declarationMap": true,
    "removeComments": false,
    "baseUrl": ".",
    "paths": {
      "@reown/appkit": ["../appkit"],
      "@reown/appkit/*": ["../appkit/*"],
      "@reown/appkit-adapter-ethers": ["../adapters/ethers"],
      "@reown/appkit-adapter-ethers/*": ["../adapters/ethers/*"],
      "@reown/appkit-adapter-bitcoin": ["../adapters/bitcoin"],
      "@reown/appkit-adapter-bitcoin/*": ["../adapters/bitcoin/*"],
      "@reown/appkit-adapter-polkadot": ["../adapters/polkadot"],
      "@reown/appkit-adapter-polkadot/*": ["../adapters/polkadot/*"],
      "@reown/appkit-adapter-solana": ["../adapters/solana"],
      "@reown/appkit-adapter-solana/*": ["../adapters/solana/*"],
      "@reown/appkit-adapter-wagmi": ["../adapters/wagmi"],
      "@reown/appkit-adapter-wagmi/*": ["../adapters/wagmi/*"],
      "@reown/appkit-cdn": ["../cdn"],
      "@reown/appkit-cdn/*": ["../cdn/*"],
      "@reown/appkit-cli": ["../cli"],
      "@reown/appkit-cli/*": ["../cli/*"],
      "@reown/appkit-common": ["../common"],
      "@reown/appkit-common/*": ["../common/*"],
      "@reown/appkit-core": ["../core"],
      "@reown/appkit-core/*": ["../core/*"],
      "@reown/appkit-experimental": ["../experimental"],
      "@reown/appkit-experimental/*": ["../experimental/*"],
      "@reown/appkit-polyfills": ["../polyfills"],
      "@reown/appkit-polyfills/*": ["../polyfills/*"],
      "@reown/appkit-scaffold-ui": ["../scaffold-ui"],
      "@reown/appkit-scaffold-ui/*": ["../scaffold-ui/*"],
      "@reown/appkit-siwe": ["../siwe"],
      "@reown/appkit-siwe/*": ["../siwe/*"],
      "@reown/appkit-siwx": ["../siwx"],
      "@reown/appkit-siwx/*": ["../siwx/*"],
      "@reown/appkit-ui": ["../ui"],
      "@reown/appkit-ui/*": ["../ui/*"],
      "@reown/appkit-ui-new": ["../ui-new"],
      "@reown/appkit-ui-new/*": ["../ui-new/*"],
      "@reown/appkit-utils": ["../appkit-utils"],
      "@reown/appkit-utils/*": ["../appkit-utils/*"],
      "@reown/appkit-wallet": ["../wallet"],
      "@reown/appkit-wallet/*": ["../wallet/*"],
      "@reown/appkit-wallet-button": ["../wallet-button"],
      "@reown/appkit-wallet-button/*": ["../wallet-button/*"]
    }
  },
  "include": [
    "exports",
    "src",
    "package.json"
  ],
  "references": [
    { "path": "../adapters/ethers" },
    { "path": "../appkit" },
    { "path": "../appkit-utils" },
    { "path": "../common" },
    { "path": "../core" },
    { "path": "../polyfills" },
    { "path": "../scaffold-ui" },
    { "path": "../ui" },
    { "path": "../wallet" },
  ]
}
