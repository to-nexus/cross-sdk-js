{
  "name": "@to-nexus/appkit-cdn",
  "version": "1.0.2",
  "main": "dist/wagmi.js",
  "type": "module",
  "files": [
    "dist",
    "!tsconfig.tsbuildinfo"
  ],
  "scripts": {
    "build:clean": "rm -rf dist",
    "build": "tsc && vite build",
    "watch": "tsc --watch",
    "upload": "pnpm build && pnpm publish --public",
    "serve": "http-server dist -p 8080 --cors"
  },
  "devDependencies": {
    "@wagmi/connectors": "5.7.7",
    "http-server": "14.1.1",
    "vite-plugin-node-polyfills": "0.23.0"
  },
  "peerDependencies": {
    "typescript": "5.7.3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@to-nexus/appkit": "~1.0.0",
    "@to-nexus/appkit-adapter-ethers": "~1.0.0",
    "@to-nexus/appkit-adapter-solana": "~1.0.0",
    "@to-nexus/appkit-adapter-wagmi": "~1.0.0",
    "@reown/appkit-polyfills": "workspace:*",
    "@wagmi/connectors": "5.7.7",
    "@wagmi/core": "2.16.4",
    "viem": "2.23.0",
    "vite": "5.4.12",
    "wagmi": "2.14.9"
  }
}
